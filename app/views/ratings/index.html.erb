<h2>List of ratings</h2>

<p>Number of ratings: <%= @ratings.count%></p>

<div id="ratings">
    <ul>
        <% @ratings.each do |rating| %>
            <li> <%= render rating %> </li>
        <% end %>
    </ul>
</div>
<%=link_to "Rate a beer", new_rating_path%>
<ul>
  <% @ratings.each do |rating| %>
    <li> <%= render rating %> <%= button_to 'delete', rating, method: :delete, form: { data: { turbo_confirm: 'Do you want to delete this rating?' } } %> </li>
  <% end %>
</ul>