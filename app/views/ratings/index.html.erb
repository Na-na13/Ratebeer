<h2>List of ratings</h2>

<p>Number of ratings: <%= @ratings.count%></p>

<div id="ratings">
    <ul>
        <% @ratings.each do |rating| %>
            <li> <%= render rating %> </li>
        <% end %>
    </ul>
</div>
<%=link_to "Rate a beer", new_rating_path, class: "btn btn-primary"%>
<ul>
  <% @ratings.each do |rating| %>
    <li> <%= render rating %> <%= link_to rating.user.username, rating.user %> </li>
  <% end %>
</ul>